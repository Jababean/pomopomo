{
  "name": "Podoro Timer",
  "version": "0.9.3",
  "manifest_version": 3,
  "description":  "A pomodoro timer, built into your browser!",
  "icons": {
    "64":"icons/pomo64.png",
    "128":"icons/pomo128.png"
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "action": {
    "default_popup": "/popup/popup.html",
    "default_icon": {
      "64":"icons/pomo64.png"
    }
  },
  "background": {
    "service_worker": "/scripts/background.js",
    "type": "module"
  }
}