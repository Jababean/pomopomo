{
  "name": "Podoro Timer",
  "version": "0.9.2",
  "manifest_version": 3,
  "description":  "A pomodoro timer, built into your browser!",
  "icons": {
    "64":"icons/pomo64.png",
    "128":"icons/pomo128.png"
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "action": {
    "default_popup": "/popup/popup.html"
  },
  "background": {
    "service_worker": "/scripts/background.js",
    "type": "module"
  }
}