{
  "name": "Pomopomo",
  "version": "0.9",
  "manifest_version": 3,
  "description":  "A simple pomodoro timer!",
  "icons": {
    "16":"icons/pomo16.png",
    "32":"icons/pomo32.png",
    "64":"icons/pomo64.png",
    "128":"icons/pomo128.png"
  },
  "permissions": [
    "notifications",
    "alarms",
    "storage"
  ],
  "action": {
    "default_popup": "/popup/popup.html"
  },
  "background": {
    "service_worker": "/scripts/background.js",
    "type": "module"
  }
}